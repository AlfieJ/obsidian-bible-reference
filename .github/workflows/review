name: "Build"

on:
  pull_request:
    # The branches below must be a subset of the branches above
    branches: [ master ]

jobs:
  build:
    runs-on: ubuntu-latest
    strategy:
      matrix:
        node-version: [14.x]

    steps:
      - name: Codeball AI Actions
        uses: sturdy-dev/codeball-action@v1.2.0

